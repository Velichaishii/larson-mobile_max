<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{% block pageTitle %}{% endblock %}</title>

    <!-- Stylesheets -->
    {% include "stylesheets.twig" %}
</head>
<body>
    <div class="wrap">
        <a class="wrap__overlay" href="#"></a>
        <a class="wrap__substrate-tp" href="#"></a>
        <!-- Header block -->
        {% include "header.twig" with {"check_back_btn": back_btn} %}
        <!-- Header block end -->

        <!-- Block content -->
        {% block content %}{% endblock %}
        <!-- Block content end -->

        <!-- Footer block -->
        {% include "footer.twig" %}
        <!-- Footer block end -->

        {% include "../widgets/burger-menu.twig" %}

        <!-- Scripts -->
        {% include "scripts.twig" %}

        {% include "popups.twig" %}
    </div>
</body>
</html>